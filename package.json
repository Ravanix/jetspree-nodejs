{
  "name": "jetspree-nodejs",
  "version": "1.0.0",
  "private": true,
  "description": "JetSpree API",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src/app --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist/app",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "samuel.lee",
  "dependencies": {
    "express": "~4.15.2",
    "express-session": "~1.15.1",
    "express-validator": "~3.1.2",
    "validator": "~7.0.0",
    "helmet": "~3.5.0",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.1",
    "debug": "~2.6.3",
    "pug": "~2.0.0-beta11",
    "stylus": "0.54.5",
    "mongodb": "2.2.25",
    "monk": "4.0.0",
    "request": "2.81.0",
    "pg": "6.1.5",
    "jsonwebtoken": "7.3.0",
    "bcrypt": "1.0.2",
    "passport": "~0.3.2",
    "passport-facebook": "~2.1.1",
    "passport-google-oauth": "~1.0.0",
    "connect-flash": "~0.1.1",
    "babel-polyfill": "~6.23.0"
  },
  "engines": {
    "node": "6.2.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.17.1",
    "nodemon": "^1.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evo0705/jetspree-nodejs.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/evo0705/jetspree-nodejs/issues"
  },
  "homepage": "https://github.com/evo0705/jetspree-nodejs#readme"
}
